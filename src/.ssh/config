# Global Section
# Must appear at the end, because EARLIER sections override LATER sections.
Host *
    Compression yes
    ExitOnForwardFailure yes
    ForwardX11Trusted no
    HashKnownHosts no
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_ed25519
    PasswordAuthentication no
    SendEnv LANG LC_*
    ServerAliveInterval 60
    TCPKeepAlive no
